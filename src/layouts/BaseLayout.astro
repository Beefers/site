---
import "virtual:windi.css";

const { title, class: className } = Astro.props;
---

<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>
    <slot name="head" />
</head>

<body class={className}>
    <slot />
</body>

</html>